{
    "name": "funddy/yodo",
    "description": "Simple, fast and customizable HTML sanitizer",
    "keywords": ["sanitizer", "html"],
    "homepage": "https://github.com/funddy/yodo",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Keyvan Akbary",
            "email": "keyvan@funddy.com"
        }
    ],
    "require": {
        "php": ">=5.3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.7.2"
    },
    "suggest": {
        "ext-tidy": "Allows HTML DOM fixing"
    },
    "autoload": {
        "psr-0": { "Funddy\\Yodo": "" }
    },
    "target-dir": "Funddy/Yodo",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
